    private void jButtonBersihkanActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        txtnama.setText("");
        txtTTL.setText("");
        txtemail.setText("");
        txtagama.setText("");
        txtalamat.setText("");
        txtNOP.setText("");
        txtoutput.setText(""); 
    }                                                

    private void jButtonKeluarActionPerformed(java.awt.event.ActionEvent evt) {                                              
        dispose();
    }                                             

    private void jButtonProsesActionPerformed(java.awt.event.ActionEvent evt) {                                              
        String jenkel = "", p , waktu;
        if (jRadioButtonPR.isSelected()) {
            jenkel = "Perempuan";
        } else if (jRadioButtonLK.isSelected()) {
            jenkel = "Laki - Laki";
        }
        
        waktu = jComboBoxWaktu.getSelectedItem().toString();
        p = jComboBoxprog.getSelectedItem().toString();
        
        txtoutput.setText("Nama: " + txtnama.getText() + "\nTempat Tanggal Lahir\t: " + txtTTL.getText() + "\nJenis Kelamin\t: " + jenkel
                + "\nAgama\t: " + txtagama.getText() + "\nNo. Telpona\t: " + txtNOP.getText() + "\nEmail\t: " + txtemail.getText()
                + "\nAlamat\t: " + txtalamat.getText() + "\nProgram Bimbingan\t: " + p + "\nWaktu Bimbingan\t: " + waktu );
    }                                             

